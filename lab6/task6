using System;

namespace task5
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите А");
            double A = Convert.ToDouble(Console.ReadLine());
            double C = A * A;
            A = C * C;
            A = A * A;
            Console.WriteLine($"A^8 = {A}");
        }
    }
}
