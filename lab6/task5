using System;

namespace task5
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Нахождение значения функции y=4*(x-3)^6-7*(x-3)^2+2");
            Console.WriteLine("Введите x");
            double x = Convert.ToDouble(Console.ReadLine());
            double a = (x + 3) * (x + 3);
            double b = a * a;
            b = a * b;
            double y = 4 * b - 7 * a + 2;
            Console.WriteLine($"y = {y}");
        }
    }
}
