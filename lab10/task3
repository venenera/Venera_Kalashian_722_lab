using System;

namespace task3
{
    class Program
    {
        static void Main(string[] args)
        {
            int A;
            {
                Console.Write("Введите целое положительное число А = ");
                A = Convert.ToInt32(Console.ReadLine());
            }

            bool condition = A > 9 && A < 100 && A % 2 == 0;

            if (condition)
            {
                Console.WriteLine("Высказвание «Данное число является четным двузначным» истинно");
            }
            else
            {
                Console.WriteLine("Высказвание «Данное число является четным двузначным» ложно");
            }
        }
    }
}
