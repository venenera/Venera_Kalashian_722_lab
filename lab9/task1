using System;

namespace lab9
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите количество секунд, прошедших с начала суток N = ");

            int N, sec;
            {
                N = Convert.ToInt32(Console.ReadLine());

                sec = N % 60;

                Console.WriteLine($"{sec} секунд прошло с начала последней минуты");
            }
        }
    }
}
