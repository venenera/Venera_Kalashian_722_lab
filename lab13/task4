using System;

namespace task3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите целое число N = ");
            int N = Convert.ToInt32(Console.ReadLine());
            int i, sum = 0;
            for (i = 1; i <= 2*N - 1; i += 2)
            {
                sum += i;
                Console.WriteLine(sum);
            }
        }
    }
}
