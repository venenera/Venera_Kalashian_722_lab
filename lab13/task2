using System;

namespace task1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите целое число  ");
            double S = Convert.ToDouble(Console.ReadLine());

            double i, C = 1 / 10, product = 1;

            for (i = 1; i < 10; ++i)
            {
                product *= S + i / 10;
            }

            Console.Write($"Произведение сомножителей равно {product}");
        }
    }
}
